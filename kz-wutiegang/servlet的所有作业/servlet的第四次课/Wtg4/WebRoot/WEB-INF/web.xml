<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<listener>
		<listener-class>com.wtg.Listener.RequestLtr</listener-class>
	</listener>
	<listener>
		<listener-class>com.wtg.Listener.ContextLtr</listener-class>
	</listener>
	<listener>
		<listener-class>com.wtg.Listener.SessionLtr</listener-class>
	</listener>
	  <listener>
		<listener-class>com.wtg.Listener.applicationLtr</listener-class>
	</listener>
	<listener>
		<listener-class>com.wtg.Listener.RequestListener</listener-class>
	</listener>
	<listener>
		<listener-class>com.wtg.Listener.SessionListener</listener-class>
	</listener>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
